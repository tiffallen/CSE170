	<div class="col-xs-12 col-md-12">

	<!-- WHERE THE PICTURES START -->
	<div class = "picturebody container" style = "margin-top: 10px;" action="/selectEvent">
		<div class = "row">
			{{#each mybucketlist}}
			{{#if delete}}
			{{else}}
			<div class = "col-xs-6 col-md-6" >
				<a class="select" id="bucketExpanded{{@index}}">
				<div class = "thumbnail img-thumbnail  module mid" style="background: 
    				linear-gradient(
      					rgba(0, 0, 0, 0.3),
      					rgba(0, 0, 0, 0.3)),
     				url({{adventurepics}});
     				background-size: cover">
					<div class = "caption text-center">
						<h4> {{adventurename}} </h4>
					</div>
				</div> 
				</a>
			</div>
			{{/if}}
			{{/each}}
		</div>
	</div>
		<br><br><br><br><br>
	</div>
