		<div class = "container1" >
			<h1> Memories </h1>
		</div>
	</div>
	
	<nav class="navbar navbar-default">
		<div class="container-fluid">
		<!-- Collect the nav links, forms, and other content for toggling -->
			<div class="collapse navbar-collapse  container1">
				<div class="nav navbar-nav navigation">
					<li class  = "col-xs-3"> <a href ="home1" onclick="__gaTracker('send', 'event', 'buttons', 'click', '1: memories->back');"> Back </a> </li>

					<li class  = "col-xs-6 text-center" >
						<form class="navbar-form nav-center">
						<div class="form-group">
							<input type="text" class="form-control" placeholder="Search my Memories">
						</div>
						<button type="submit" class="btn btn-secondary">Search </button>
						</form>
					</li>
					<li class  = "col-xs-3 .col-md-offset-3"\ >
						<ul class = "nav navbar-nav">
							<li> <a href = "newMemory1" style="font-size:100%" onclick="__gaTracker('send', 'event', 'buttons', 'click', '1: memories->new');"> New Memory Event</a> </li>
						</ul>
					</li>
					<!-- 
					<li class  = "col-xs-3 .col-md-offset-3"\ >
						<ul class = "nav navbar-nav">
							<li> <a href = "newMemCategory"> New Memory Folder</a> </li>
						</ul>
					</li>
					-->
					<li class  = "col-xs-3 .col-md-offset-3"\ >
						
					</li>
				</div>
				
				
			</div><!-- /.navbar-collapse -->
		</div><!-- /.container-fluid -->
	</nav>
	
	<div class="col-xs-12 col-md-12">

	<!-- WHERE THE PICTURES START -->
	<div class = "picturebody pagebottom container">
		<div class = "row">
			{{#each mymemories}}
			<div class = "col-xs-6 col-md-6" >
				<a id="memExpanded1{{@index}}">
				<div class = "thumbnail img-thumbnail  module mid" style="background: 
    				linear-gradient(
      					rgba(0, 0, 0, 0.3),
      					rgba(0, 0, 0, 0.3)),
     				url({{trippics}});
     				background-size: cover">
					<div class = "caption text-center">
						<h4> {{tripname}} </h4>
					</div>
				</div> 
				</a>
			</div>
			{{/each}}
		</div>
	</div>
	<br><br><br>
	</div>
	<!-- 	FOOTER 		-->
	<nav id="footer" class = "footer nav navbar-default" >
		<div class = "container-fluid container2 row-fluid">
			<div class="col-xs-1"></div>
			<li> <a href ="home1" onclick="__gaTracker('send', 'event', 'buttons', 'click', 'memories->home');" class = "col-xs-4"> Home <span class = "sr-only"> </span></a> </li>

			<li> <a href ="memories1" onclick="__gaTracker('send', 'event', 'buttons', 'click', 'memories->memories');" class = "col-xs-4"> Memories <span class = "sr-only"> </span></a> </li>

			<li> <a href ="bucketlist1" onclick="__gaTracker('send', 'event', 'buttons', 'click', 'memories->bucket');" class = "col-xs-4"> Bucketlist <span class = "sr-only"> </span></a> </li>
		</div>
	</nav>
	<script src="javascripts/button_interactions/mem_interaction1.js"></script>

