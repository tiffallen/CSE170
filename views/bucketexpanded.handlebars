.<!document html>
<html>
<head> 
	<title>Explore Category Selected</title>
	<meta charset="utf-8"/>
	<meta name="viewport" content="width=device-width, initial-scale=1.0"/>
	<link href="stylesheets/bootstrap.min.css" rel="stylesheet"/>
	<link href="stylesheets/bootstrap-theme.min.css" rel="stylesheet"/>
	<link href="stylesheets/memories.css" rel="stylesheet"/>
	
	<link href="https://fonts.googleapis.com/css?family=Shadows+Into+Light" rel="stylesheet">
	
	<script src="javascripts/jquery-1.11.0.js"></script>
	<script src="javascripts/bucketlist.js"></script>
	
</head>

<body>
	<div class = "jumbotron app-name page-header">
		<div class = "container1" >
			<h1> Bucket List Expanded Handlebar </h1>
		</div>
	</div>
	
	<nav class="navbar navbar-default">
		<div class="container-fluid">
		<!-- Collect the nav links, forms, and other content for toggling -->
			<div class="collapse navbar-collapse  container1">
				<div class="nav navbar-nav navigation">
					<!--<li class  = "col-xs-3"> <a href ="home" > Back </a> </li>
					Don't need a back button	-->
					<li class  = "col-xs-12 text-center" >
					</li>
					<li class  = "col-xs-3 .col-md-offset-3"\ >
						
					</li>
				</div>
				
				
			</div><!-- /.navbar-collapse -->
		</div><!-- /.container-fluid -->
	</nav>

	<!-- WHERE THE PICTURES START -->
	{{#each mybucketlist}}
	{{#if selected}}
	{{else}}
	<div class = "picturebody container " style = "margin-top: 10px;">
			<div class = "col-xs-3 col-md-3" > </div>
			<div class = "col-xs-6 col-md-6" >
				<div class = "thumbnail img-thumbnail  module mid" style="background: 
    				linear-gradient(
      					rgba(0, 0, 0, 0.3),
      					rgba(0, 0, 0, 0.3)),
     				url({{adventurepics}});
     				background-size: cover">
					<div class = "caption text-center">
						<h4> {{adventurename}} </h4>
					</div>
				</div>
			</div>
			<div class = "col-xs-4 col-md-4">
					<br><br>
			</div> 
	</div>
	{{/if}}
	{{/each}}

	<hr size="20">

	<div class = "col-xs-2 col-md-2" ></div>
	<div class = "col-xs-3 col-md-3" >

		<h1 class="text-center" style="font-family: 'Shadows Into Light', cursive;"> Category </h1> 
		<h1 class="text-center" style="font-family: 'Shadows Into Light', cursive;"> Location </h1> 
  </div>
  <div class = "col-xs-3 col-md-3">

		<h3 style="font-size: 30px"> Outdoors </h3>
		<h3 style="font-size: 30px"> Camp Pendleton, Oceanside </h3><br>

		<br><br><br><br>
  </div>

	<!-- 	FOOTER 		-->
	<nav class = "footer nav navbar-default" >
		<div class = "container-fluid container2">
			<li> <a href ="home" class = "col-xs-4"> Home <span class = "sr-only"> </span></a> </li>
			<li> <a href = "memories" class = "col-xs-4"> Memories <span class = "sr-only"> </span> </a> </li>
			<li> <a href = "bucketlist" class = "col-xs-4"> Bucketlist <span class = "sr-only"> </span> </a> </li>
		</div>
	</nav>

</body>
</html>
