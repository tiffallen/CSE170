.<!document html>
<html>
<head>

	<title>Explore {{event}}</title>
	<meta charset="utf-8"/>
	<meta name="viewport" content="width=device-width, initial-scale=1.0"/>
	<link href="stylesheets/bootstrap.min.css" rel="stylesheet"/>
	<link href="stylesheets/bootstrap-theme.min.css" rel="stylesheet"/>
	<link href="stylesheets/home.css" rel="stylesheet"/>
	
	<link href="https://fonts.googleapis.com/css?family=Shadows+Into+Light" rel="stylesheet">

	<link rel="stylesheet" href="http://www.w3schools.com/lib/w3.css">
	
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
	<!-- <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/public/stylesheets/bootstrap.min.js"></script> -->
	
</head>

<body>
	<div class = "jumbotron app-name page-header header">
		<img src="/images/Moments1.png">
	</div>
	
	<nav class="navbar navbar-default">
		<div class="container-fluid">
		<!-- Collect the nav links, forms, and other content for toggling -->
			<div class="collapse navbar-collapse  container1">
				<div class="nav navbar-nav navigation">
					<li class  = "col-xs-2 col-md-offset-2"\>
						<ul class = "nav navbar-nav">
							<li> <a id="homeExpanded{{index}}" onclick="ga('send', 'event', 'bbuttons', 'click', '0: homeEvent->back');"><input type="image" class="nav-top-img" src="../../images/backbutton.png"> </input></a> </li>
						</ul>
					</li>
					<li class="col-xs-1 col-md-offset-1"\>
					
					<li class  = "col-xs-6 text-center" >
						<h2 style="text-decoration: underline;color:black;font-family: 'Shadows Into Light', cursive; " > {{event}} </h2>
					</li>
					<li class  = "col-xs-3 .col-md-offset-3"\ >
						
					</li>
				</div>
				
				
			</div><!-- /.navbar-collapse -->
		</div><!-- /.container-fluid -->
	</nav>


	<!-- WHERE THE PICTURES START -->
	<div class = "picturebody container col-xs-12 col-md-12 pagebottom" style = "margin-top: 10px;">
		<div class="w3-content w3-display-container col-xs-10 col-md-10 pic-center img-background">
  			<img class="mySlides" src="{{event_url}}" style="height:120%;">
  			<img class="mySlides" src="{{pic1}}" style="height:120%;">
  			<img class="mySlides" src="{{pic2}}" style="height:120%;">
  			<img class="mySlides" src="{{pic3}}" style="height:120%;">
  			<img class="mySlides" src="{{pic4}}" style="height:120%;">

  			<a class="w3-btn-floating w3-display-left" onclick="plusDivs(-1)">&#10094;</a>
  			<a class="w3-btn-floating w3-display-right" onclick="plusDivs(1)">&#10095;</a>
  			<br><br>
		</div>
		<script>
			var slideIndex = 1;
			showDivs(slideIndex);

			function plusDivs(n) {
			  showDivs(slideIndex += n);
			}

			function showDivs(n) {
			  var i;
			  var x = document.getElementsByClassName("mySlides");
			  if (n > x.length) {slideIndex = 1}    
			  if (n < 1) {slideIndex = x.length}
			  for (i = 0; i < x.length; i++) {
			     x[i].style.display = "none";  
			  }
			  x[slideIndex-1].style.display = "block";  
			}
		</script>


			<div class = "col-xs-7 col-md-7" >
				<form id="addEventForm" role="form" method="get" action="/addEvent">
	     		<div class="col-xs-12 col-md-12">
				
				<br>
				<a class="btn btn-default" style="color: black" href="{{googlemaps}}"> Open Google Maps</a>
				<br><br>
				<a class="text-center btn btn-default" style="color: black" href="{{website}}" target="_blank"> Website Link </a>

	     		<div class="form-group">
					<input type="hidden" name="adventurename" value="{{event}}">
					<input type="hidden" name="category" value="{{category}}">
					<input type="hidden" name="locationideas" value="{{event}}">
					<input type="hidden" name="adventurepics" value="{{event_url}}">
	     		</div>

					<input type="submit" id="submitBtn" class="btn btn-success" value="Add to Bucketlist" onclick="location.href='/bucketlist'"></input>
					<br><br><br><br><br><br><br><br><br><br><br><br><br><br><br><br>
				</div>

			</form> 
			</div>
			<div class = "col-xs-5 col-md-5">
				
					<h4 style="text-decoration: underline;"> Address: </h4> <h6> &nbsp;&nbsp;{{location}} </h6>
					<h4 style="text-decoration: underline;"> Neighborhood: </h4> <h6 > &nbsp;&nbsp;{{neighborhood}} </h6>
			</div> 
		
	</div>
	


	<!-- 	FOOTER 		-->
	<nav id="footer" class = "footer nav navbar-default" >
		<div class = "container-fluid container2 row-fluid">
			<div class="col-xs-1"></div>
			<li class="footer-border footer-seperation"> <a href ="home" onclick="ga('send', 'event', 'fbuttons', 'click', '0: home->home');" class = "col-xs-4 offset1"> <input type="image" class="footer-img" src="../../images/homebutton.png"> Home <span class = "sr-only"> </span></input></a> </li>

			<li class="footer-border footer-seperation"> <a href = "memories" onclick="ga('send', 'event', 'fbuttons', 'click', '0: home->memory');" class = "col-xs-4 te"> <input type="image" class="footer-img" style="padding-top:3px;" src="../../images/memoriesbutton.png">Memories <span class = "sr-only"> </span> </input></a> </li>

			<li class="footer-seperation"> <a href = "bucketlist" onclick="ga('send', 'event', 'fbuttons', 'click', '0: home->bucket');" class = "col-xs-4"><input type="image" class="footer-img" src="../../images/bucketlistbutton.png"> Bucketlist <span class = "sr-only"> </span></input></a> </li>
		</div>
	</nav>

<script>
